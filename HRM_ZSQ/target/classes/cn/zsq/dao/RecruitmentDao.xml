<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.zsq.dao.RecruitmentDao">
    <select id="getRecruitments" resultType="cn.zsq.model.Recruitment">
            select * from  RECRUITMENT
    </select>
    <select id="getRecruitmentsByAny" resultType="cn.zsq.model.Recruitment">
        select * from  RECRUITMENT
        <where>
            RM_ID = #{rmId}
        </where>
    </select>
        </mapper>